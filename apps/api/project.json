{
    "name": "api",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "projectType": "application",
    "sourceRoot": "apps/api/src",
    "targets": {
        "build": {
            "executor": "nx:run-script",
            "options": {
                "script": "build"
            }
        },
        "serve": {
            "executor": "nx:run-script",
            "options": {
                "script": "start:dev"
            }
        },
        "lint": {
            "executor": "nx:run-script",
            "options": {
                "script": "lint"
            }
        },
        "test": {
            "executor": "nx:run-script",
            "options": {
                "script": "test"
            }
        }
    },
    "tags": [
        "type:app",
        "scope:backend"
    ]
}