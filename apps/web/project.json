{
    "name": "web",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "projectType": "application",
    "sourceRoot": "apps/web/src",
    "targets": {
        "build": {
            "executor": "nx:run-script",
            "options": {
                "script": "build"
            }
        },
        "serve": {
            "executor": "nx:run-script",
            "options": {
                "script": "dev"
            }
        },
        "lint": {
            "executor": "nx:run-script",
            "options": {
                "script": "lint"
            }
        },
        "preview": {
            "executor": "nx:run-script",
            "options": {
                "script": "preview"
            }
        }
    },
    "tags": [
        "type:app",
        "scope:frontend"
    ]
}